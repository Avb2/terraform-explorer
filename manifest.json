{
  "manifest_version": 3,
  "name": "Terraform Resource Explorer",
  "version": "1.1.0",
  "description": "Visualize Terraform configurations as interactive dependency trees with expandable hierarchy, resource details, and dependency highlighting.",
  "permissions": [],
  "host_permissions": [
    "https://github.com/*",
    "https://github.dev/*"
  ],
  "content_scripts": [{
    "matches": ["https://github.com/*", "https://github.dev/*"],
    "js": ["src/lib/cytoscape.min.js", "src/js/content.js", "src/js/script.js"],
    "run_at": "document_idle"
  }],
  "web_accessible_resources": [{
    "resources": [
      "src/index.html",
      "src/lib/*"
    ],
    "matches": ["https://github.com/*", "https://github.dev/*"]
  }],
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon96.png",
    "128": "assets/icons/store-icon-128.png"
  }
}
